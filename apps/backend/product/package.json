    "sequelize-cli": "^6.6.2"
  },
  "devDependencies": {
    "bun-types": "latest",
    "pg": "^8.13.1",
    "sequelize": "^6.37.5",
    "sequelize-typescript": "^2.1.6"
  },
  "module": "src/index.js"
}